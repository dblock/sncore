           <img src="images/spinner.gif" width="16" height="16" alt="Working ..." />
          </ProgressTemplate>
         </atlas:UpdateProgress>
        </div>
        <!-- NOEMAIL-END -->
       </td>
      </tr>
      <tr>
       <td colspan="2">
