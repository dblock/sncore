           <asp:ListItem>/AccountResetPassword.aspx</asp:ListItem>
          </Highlights>
         </SnCore:MenuItem><SnCore:MenuItem 
          runat="server" ID="menuMe" Text="me me" NavigateUrl="AccountPreferencesManage.aspx">
