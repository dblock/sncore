          <SnCore:Notice ID="noticeMenu" EnableViewState="false" runat="server" />
         </ContentTemplate>
        </atlas:UpdatePanel>
        <!-- NOEMAIL-END -->
        <asp:ContentPlaceHolder ID="ContentPlaceHolder1" runat="server">
        </asp:ContentPlaceHolder>
