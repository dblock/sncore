<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.0" schema="dbo">
  <class name="PictureType, SnCore.Data" table="PictureType">
    <id access="nosetter.pascalcase-m-underscore"  name="Id" type="Int32" column="PictureType_Id">
      <generator class="identity" />
    </id>
    <property name="Name" column="Name" type="String" />
    <bag lazy="true" name="Pictures" inverse="true">
      <key column="Type" />
      <one-to-many class="Picture, SnCore.Data" />
    </bag>
  </class>
</hibernate-mapping>
