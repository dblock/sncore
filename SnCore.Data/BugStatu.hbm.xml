<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.0" schema="dbo">
  <class name="BugStatu, SnCore.Data" table="BugStatus">
    <id access="nosetter.pascalcase-m-underscore"  name="Id" type="Int32" column="BugStatus_Id">
      <generator class="identity" />
    </id>
    <bag lazy="true" name="Bugs" inverse="true">
      <key column="Status_Id" />
      <one-to-many class="Bug, SnCore.Data" />
    </bag>
    <property name="Name" column="Name" type="String" />
  </class>
</hibernate-mapping>
