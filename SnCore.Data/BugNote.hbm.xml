<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.0" schema="dbo">
  <class name="BugNote, SnCore.Data" table="BugNote">
    <id access="nosetter.pascalcase-m-underscore"  name="Id" type="Int32" column="BugNote_Id">
      <generator class="identity" />
    </id>
    <property name="AccountId" column="Account_Id" type="Int32" />
    <many-to-one name="Bug" column="Bug_Id" class="Bug, SnCore.Data" />
    <property name="Created" column="Created" type="DateTime" />
    <property name="Details" column="Details" type="String" />
    <property name="Modified" column="Modified" type="DateTime" />
  </class>
</hibernate-mapping>
