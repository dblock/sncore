<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.0" schema="dbo">
  <class name="AccountPicture, SnCore.Data" table="AccountPicture">
    <id access="nosetter.pascalcase-m-underscore"  name="Id" type="Int32" column="AccountPicture_Id">
      <generator class="identity" />
    </id>
    <many-to-one name="Account" column="Account_Id" class="Account, SnCore.Data" />
    <property name="Bitmap" column="Bitmap" type="BinaryBlob" length="1048576" />
    <property name="Created" column="Created" type="DateTime" />
    <property name="Description" column="Description" type="String" />
    <property name="Modified" column="Modified" type="DateTime" />
    <property name="Name" column="Name" type="String" />
  </class>
</hibernate-mapping>
