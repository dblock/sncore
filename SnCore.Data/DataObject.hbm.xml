<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.0" schema="dbo">
  <class name="DataObject, SnCore.Data" table="DataObject">
    <id access="nosetter.pascalcase-m-underscore"  name="Id" type="Int32" column="DataObject_Id">
      <generator class="identity" />
    </id>
    <property name="Name" column="Name" type="String" />
    <bag lazy="true" name="AccountRights" inverse="true">
      <key column="DataObject_Id" />
      <one-to-many class="AccountRight, SnCore.Data" />
    </bag>
    <bag lazy="true" name="Reminders" inverse="true">
      <key column="DataObject_Id" />
      <one-to-many class="Reminder, SnCore.Data" />
    </bag>
  </class>
</hibernate-mapping>
