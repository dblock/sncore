<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.0" schema="dbo">
  <class name="BugSeverity, SnCore.Data" table="BugSeverity">
    <id access="nosetter.pascalcase-m-underscore"  name="Id" type="Int32" column="BugSeverity_Id">
      <generator class="identity" />
    </id>
    <bag lazy="true" name="Bugs" inverse="true">
      <key column="Severity_Id" />
      <one-to-many class="Bug, SnCore.Data" />
    </bag>
    <property name="Name" column="Name" type="String" />
  </class>
</hibernate-mapping>
